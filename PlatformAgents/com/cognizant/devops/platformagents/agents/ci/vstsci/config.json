{
  "mqConfig": {
    "user": "iSight",
    "password": "iSight",
    "host": "127.0.0.1",
    "exchange": "iSight",
    "agentControlXchg": "iAgent"
  },
  "subscribe": {
    "config": "CI.VSTS.config"
  },
  "publish": {
    "data": "CI.VSTS.DATA",
    "health": "CI.VSTS.HEALTH"
  },
  "communication": {
    "type": "REST",
    "sslVerify": true,
    "responseType": "JSON"
  },
  "dynamicTemplate": {
    "timeFieldMapping": {
      "startDate": "%Y-%m-%d"
    },
    "responseTemplate": {
      "result": "result",
      "id": "buildId",
      "sourceVersion": "sourceVersion",
      "queueTime": "queuedDate",
      "reason": "manual",
      "requestedBy": {
        "displayName": "ciAuthorName",
        "uniqueName": "ciAuthor"
      },
      "status": "status",
      "repository": {
        "type": "repository"
      },
      "buildNumber": "buildNumber",
      "startTime": "startDate",
      "finishTime": "finishDate",
      "definition": {
        "name": "buildDefinitionName"
      },
      "url": "buildUrl",
      "priority": "priority"
    }
  },
  "toolCategory": "CI",
  "enableDataValidation": true,
  "isDebugAllowed": false,
  "enableValueArray": false,
  "auth": "NTLM",
  "toolsTimeZone": "Asia/Kolkata",
  "insightsTimeZone": "Asia/Kolkata",
  "useResponseTemplate": true,
  "runSchedule": 30,
  "userID": "user",
  "passwd": "auth token",
  "baseUrl": "https://<vsts-hostname>",
  "collectionName": "myorganization",
  "timeStampField": "startDate",
  "timeStampFormat": "%Y-%m-%dT%H:%M:%S",
  "isEpochTimeFormat": false,
  "startFrom": "2017-01-01 01:01:01",
  "loggingSetting": {
    "logLevel": "WARN",
    "maxBytes": 5000000,
    "backupCount": 1000
  }
}
